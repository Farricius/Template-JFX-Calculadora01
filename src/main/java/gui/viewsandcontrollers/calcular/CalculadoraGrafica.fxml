<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="326.0"
	prefWidth="604.0" xmlns="http://javafx.com/javafx/15.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="gui.viewsandcontrollers.calcular.CalculadoraGraficaController">
	<children>
		<Label layoutX="14.0" layoutY="31.0" prefHeight="34.0"
			prefWidth="86.0" text="Número 1">
			<font>
				<Font size="13.0" />
			</font>
		</Label>
		<Label layoutX="302.0" layoutY="36.0" prefHeight="24.0"
			prefWidth="86.0" text="Número 2">
			<font>
				<Font size="13.0" />
			</font>
		</Label>
		<TextField fx:id="digitoSegundo" layoutX="393.0"
			layoutY="35.0" prefHeight="30.0" prefWidth="138.0">
			<font>
				<Font size="13.0" />
			</font>
		</TextField>
		<Label layoutX="14.0" layoutY="114.0" text="Resultado">
			<font>
				<Font size="13.0" />
			</font>
		</Label>
		<TextField fx:id="digitoResultado" editable="false"
			layoutX="100.0" layoutY="108.0" prefHeight="30.0" prefWidth="450.0">
			<font>
				<Font size="13.0" />
			</font>
		</TextField>
		<Button fx:id="botonDiv" layoutX="475.0" layoutY="179.0"
			onAction="#hazDiv" prefHeight="96.0" prefWidth="114.0" text="/"
			textAlignment="CENTER">
			<font>
				<Font size="18.0" />
			</font>
		</Button>
		<TextField fx:id="digitoPrimero" layoutX="100.0"
			layoutY="35.0" prefHeight="30.0" prefWidth="138.0">
			<font>
				<Font size="13.0" />
			</font>
		</TextField>
		<Button fx:id="botonSuma" layoutX="21.0" layoutY="179.0"
			onAction="#hazSuma" prefHeight="96.0" prefWidth="114.0" text="+"
			textAlignment="CENTER">
			<font>
				<Font size="32.0" />
			</font>
		</Button>
		<Button fx:id="botonResta" layoutX="176.0" layoutY="179.0"
			onAction="#hazResta" prefHeight="96.0" prefWidth="114.0" text="-"
			textAlignment="CENTER">
			<font>
				<Font size="32.0" />
			</font>
		</Button>
		<Button fx:id="botonMult" layoutX="320.0" layoutY="179.0"
			onAction="#hazMult" prefHeight="96.0" prefWidth="114.0" text="*"
			textAlignment="CENTER">
			<font>
				<Font size="32.0" />
			</font>
		</Button>
	</children>
</AnchorPane>
